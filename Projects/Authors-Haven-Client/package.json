    "save-dev": "^2.0.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.3",
    "sinon": "^6.2.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.5.0",
