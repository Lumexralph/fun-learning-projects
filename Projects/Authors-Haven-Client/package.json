{
  "name": "fargo-ah-client",
  "version": "1.0.0",
  "description": "[CLIENT] Authors Haven - A Social platform for the creative at heart.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel server.js -d dist",
    "start": "node ./dist/server.js",
    "dev": "webpack-dev-server --open --config webpack.dev.js ",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage -u",
    "lint": "eslint --ext .js --ext .jsx src",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/testSetup.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 30,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/fargo-ah-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/fargo-ah-client/issues"
  },
  "homepage": "https://github.com/andela/fargo-ah-client#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cors": "^2.8.4",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^6.2.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "webpack-dev-server": "^3.1.1",
    "winston": "^3.1.0"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^2.2.6",
    "axios": "^0.18.0",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "file-loader": "^2.0.0",
    "html-to-react": "^1.3.3",
    "html-webpack-plugin": "^3.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.2",
    "moxios": "^0.4.0",
    "nock": "^9.6.1",
